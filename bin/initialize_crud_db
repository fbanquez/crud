#!/home/xunil/venv/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'crud','console_scripts','initialize_crud_db'
__requires__ = 'crud'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('crud', 'console_scripts', 'initialize_crud_db')()
    )
